<mvc:View xmlns:layout="sap.ui.layout" xmlns:semantic="sap.f.semantic" controllerName="zapp.parceiros.controller.Parceiro"
    xmlns:mvc="sap.ui.core.mvc" displayBlock="true"
    xmlns="sap.m"
    xmlns:f="sap.f">
    <Page>

        <semantic:SemanticPage >
            <semantic:titleHeading>
                <Title
                    level="Auto"
                    text="{Nome1} {Nome2}"
                    textAlign="Initial"
                    titleStyle="Auto"
                    width=""
                    wrapping="false"
                    wrappingType="Normal"
                />
            </semantic:titleHeading>
            
            <semantic:headerContent>
                <layout:HorizontalLayout allowWrapping="true">
                    <ObjectAttribute title="Tipo de Parceiro" text="{Tipo}"/>
                    <ObjectAttribute title="ID Parceiro" text="{CodigoParceiro}"></ObjectAttribute>
                </layout:HorizontalLayout>
            </semantic:headerContent>
        </semantic:SemanticPage>

        <!-- <Text text="{Nome1}"></Text> -->
    </Page>
</mvc:View>
